{
  "manifest_version": 2,

  "name": "Jourmap",
  "description": "Add places to your itineraries on Jourmap",
  "version": "1.0",
  "default_locale": "en",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Add to Jourmap"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"https://material.angularjs.org/",
	"https://www.jourmap.com/",
	"https://fonts.googleapis.com/"
  ]
}
